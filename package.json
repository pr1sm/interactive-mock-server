{
  "name": "interactive-mock-server",
  "version": "0.1.0",
  "description": "Mock Server that can be configured interactively in real-time!",
  "main": "src/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/app.js",
    "start:appdev": "cross-env NODE_ENV=development nodemon src/app.js",
    "start:webdev": "cross-env NODE_ENV=development webpack-dev-server --mode=development --watch",
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --config ./.prettierrc --write \"{src,web}/**/*\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"{src,web}/**/*\"",
    "lint": "eslint \"{web,src}/**/*{.js,.jsx}\""
  },
  "author": "Srinivas Dhanwada <dhanwada.dev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "feather-icons": "^4.9.0",
    "node-fetch": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-preset-react-app": "^6.1.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.7",
    "prettier": "1.15.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
